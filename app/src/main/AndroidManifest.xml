<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.acer.oopproject">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_cinema"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.example.mirlan.oop_cinema.ActivityMain">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.example.mirlan.oop_cinema.ActivityMovieInfo" />
        <activity android:name="com.example.mirlan.oop_cinema.ActivitySeatReservation" />
        <activity android:name="com.example.mirlan.oop_cinema.ActivityReservationInfo" />
        <activity android:name="com.example.mirlan.oop_cinema.ActivitySignIn" />
        <activity android:name="com.example.mirlan.oop_cinema.ActivityUserProfile" />
    </application>

</manifest>